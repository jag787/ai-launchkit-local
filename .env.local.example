############
# PostgreSQL Performance Tuning
# Adjust these based on your server's hardware
# See: docs/POSTGRESQL_TUNING.md for detailed guidance
############

# Connection Limits
# Minimum (8GB RAM): 100 | Standard (16GB RAM): 200 | High-End (96GB RAM): 500
POSTGRES_MAX_CONNECTIONS=200

# Memory Settings
# Minimum: 256MB | Standard: 1GB | High-End: 24GB
POSTGRES_SHARED_BUFFERS=1GB

# Cache Size (should be ~75% of total RAM)
# Minimum: 2GB | Standard: 8GB | High-End: 72GB
POSTGRES_EFFECTIVE_CACHE_SIZE=8GB

# Work Memory per Connection
# Minimum: 64MB | Standard: 128MB | High-End: 256MB
POSTGRES_WORK_MEM=128MB

# Parallel Query Execution (number of CPU cores)
# Minimum: 2 | Standard: 4 | High-End: 16
POSTGRES_MAX_WORKER_PROCESSES=4

############
# Docker and System configuration
############

# Number of n8n workers for parallel processing
# Adjust based on CPU cores: Minimum: 1 | Standard: 2 | High-End: 8
N8N_WORKER_COUNT=1

# Import 300+ ready-made n8n workflows (takes ~30 minutes)
RUN_N8N_IMPORT=false

# Docker socket location
DOCKER_SOCKET_LOCATION=/var/run/docker.sock

# System timezone
TZ=Europe/Berlin

# User/Group IDs for containers
PUID=1000
PGID=1000

############
# Service Profiles - Choose which services to run
# Available profiles:
############

COMPOSE_PROFILES="n8n,dify,flowise,bolt,openui,monitoring,portainer,postiz,baserow,nocodb,vikunja,leantime,calcom,formbricks,metabase,kopia,langfuse,qdrant,weaviate,lightrag,neo4j,letta,gotenberg,stirling-pdf,crawl4ai,ragapp,open-notebook,open-webui,searxng,perplexica,python-runner,gpu-nvidia,comfyui-gpu,speech-gpu,tts-chatterbox,scriberr,ocr,libretranslate"

